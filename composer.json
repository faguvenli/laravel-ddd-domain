{
    "name": "faguvenli/laravel-ddd-domain",
    "description": "A Laravel package to create DDD structured domains",
    "keywords": ["laravel", "ddd", "domain-driven-design"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laravelddd\\Domain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravelddd\\Domain\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelddd\\Domain\\LaravelDddDomainServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
